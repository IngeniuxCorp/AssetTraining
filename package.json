{
	"name": "customtab-quickstart",
	"version": "1.0.0",
	"license": "MIT",
	"description": "Quick start project for CMS custom tab dev",
	"scripts": {
		"build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
		"build:required": "uglifyjs node_modules/core-js/client/shim.min.js node_modules/zone.js/dist/zone.js node_modules/jquery/dist/jquery.min.js node_modules/signalr/jquery.signalR.js --output ./src/modules.js --compress --mangle",
		"build:css": "copyfiles -f ./node_modules/@angular/material/prebuilt-themes/indigo-pink.css ./Content/Styles",
		"build:release": "npm run build:aot && npm run build:required && npm run build:css"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^4.2.4",
		"@angular/material": "^2.0.0-beta.10",
		"@angular/cdk": "^2.0.0-beta.10",
		"@angular/common": "^4.2.4",
		"@angular/compiler": "^4.2.4",
		"@angular/core": "^4.2.4",
		"@angular/forms": "^4.2.4",
		"@angular/http": "^4.2.4",
		"@angular/platform-browser": "^4.2.4",
		"@angular/platform-browser-dynamic": "^4.2.4",
		"@angular/router": "^4.2.4",
		"core-js": "^2.4.1",
		"rxjs": "^5.4.2",
		"zone.js": "^0.8.14",
		"jquery": "^3.2.1",
		"signalr": "^2.2.2",
		"systemjs": "0.19.40"
	},
	"devDependencies": {
		"@angular/cli": "1.4.2",
		"@angular/compiler-cli": "^4.2.4",
		"@angular/language-service": "^4.2.4",
		"@types/jasmine": "~2.5.53",
		"@types/jasminewd2": "~2.0.2",
		"@types/node": "~6.0.60",
		"codelyzer": "~3.1.1",
		"copyfiles": "^1.2.0",
		"jasmine-core": "~2.6.2",
		"jasmine-spec-reporter": "~4.1.0",
		"karma": "~1.7.0",
		"karma-chrome-launcher": "~2.1.1",
		"karma-cli": "~1.0.1",
		"karma-coverage-istanbul-reporter": "^1.2.1",
		"karma-jasmine": "~1.1.0",
		"karma-jasmine-html-reporter": "^0.2.2",
		"protractor": "~5.1.2",
		"rollup": "^0.50.0",
		"rollup-plugin-commonjs": "^8.2.1",
		"rollup-plugin-node-resolve": "^3.0.0",
		"rollup-plugin-uglify": "^2.0.1",
		"ts-node": "~3.2.0",
		"tslint": "~5.3.2",
		"typescript": "~2.3.3",
		"uglify-js": "^3.1.1"
	}
}
